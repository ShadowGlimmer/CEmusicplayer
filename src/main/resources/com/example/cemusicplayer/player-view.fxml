<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="playPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18"
            xmlns:fx="http://javafx.com/fxml/1">
   <Label fx:id="songPlayingLabel" alignment="CENTER" prefHeight="47.0" prefWidth="600.0" text="Song"
          textAlignment="CENTER">
      <font>
         <Font name="Dubai Light" size="50.0"/>
      </font>
   </Label>
   <Label fx:id="genreLabel" layoutX="14.0" layoutY="351.0" text="Genre:">
      <font>
         <Font name="Dubai Light" size="20.0"/>
      </font>
   </Label>
   <Label fx:id="artistLabel" layoutX="17.0" layoutY="316.0" text="Artist:">
      <font>
         <Font name="Dubai Light" size="20.0"/>
      </font>
   </Label>
   <Button fx:id="playButton" layoutX="270.0" layoutY="267.0" mnemonicParsing="false" text="Play">
      <font>
         <Font name="Dubai Light" size="20.0"/>
      </font>
   </Button>
   <Button fx:id="nextButton" layoutX="330.0" layoutY="279.0" mnemonicParsing="false" prefHeight="30.0"
           prefWidth="119.0" text="Next">
      <font>
         <Font name="Dubai Light" size="12.0"/>
      </font>
   </Button>
   <Button fx:id="previousButton" layoutX="151.0" layoutY="279.0" mnemonicParsing="false" prefHeight="30.0"
           prefWidth="119.0" text="Previous">
      <font>
         <Font name="Dubai Light" size="12.0"/>
      </font>
   </Button>
   <Button fx:id="continousButton" layoutX="511.0" layoutY="316.0" mnemonicParsing="false" prefHeight="75.0"
           prefWidth="75.0" text="Continous">
      <font>
         <Font name="Dubai Light" size="14.0"/>
      </font>
   </Button>
   <Slider fx:id="songVolumeSlider" layoutX="152.0" layoutY="339.0" prefHeight="30.0" prefWidth="297.0" value="50.0"/>
   <ProgressBar fx:id="songProgressBar" layoutX="57.0" layoutY="186.0" prefHeight="14.0" prefWidth="488.0"
                progress="0.0"/>
</AnchorPane>
